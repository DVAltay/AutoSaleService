@model AutoServiceSale.Entities.Concrete.Slider

@{
    ViewData["Title"] = "Delete Slide";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<h1>Delete Slide</h1>

<h3>Are you sure?</h3>
<div>
    
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Yes, Delete" class="btn btn-danger" /> |
        <a asp-action="Index" value="Cancel" class="btn btn-primary">Cancel</a>
    </form>
</div>
